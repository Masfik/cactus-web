  },
  "keywords": [],
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "start": "nuxt-ts start",
    "server-dev": "cross-env NODE_ENV=development ts-node-dev -r esm --respawn --transpileOnly server/index.ts",
    "start-server": "cross-env NODE_ENV=production ts-node -r esm server/index.ts",
    "prestart:server-dev": "npm run build",
    "prestart:start": "nuxt-ts generate && npm run build"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.3.8",
    "@nuxtjs/axios": "^5.9.5",
