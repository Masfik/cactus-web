    "esm": "^3.2.25",
    "express": "^4.17.1",
    "nuxt": "^2.11.0"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.5.6",
    "@nuxtjs/eslint-config": "^2.0.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/tailwindcss": "^1.3.2",
    "@types/express": "^4.17.2",
    "@types/node": "^13.7.1",
    "eslint": "^6.8.0",
    "eslint-plugin-nuxt": "^0.5.1",
    "ts-node": "^8.6.2",
